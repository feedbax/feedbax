{
  "name": "frontend-next",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "concurrently 'pnpm dev:scss-types' 'pnpm dev:next'",
    "dev:next": "next dev",
    "dev:scss-types": "tsm --watch src --aliasPrefixes.@ src --nameFormat param --exportType default",    
    "build": "pnpm build:i18n && pnpm build:scss-types && pnpm build:next",
    "build:next": "next build",
    "build:i18n": "pnpm ts-node src/utils/i18n/generator/index.ts",
    "build:scss-types": "tsm src --aliasPrefixes.@ src --nameFormat param --exportType default",
    "start": "next start",
    "lint": "eslint src/**/*.{js,ts,jsx,tsx}",
    "ts-node": "TS_NODE_COMPILER_OPTIONS='{\"module\":\"commonjs\"}' ts-node --compiler ttypescript"
  },
  "dependencies": {
    "@emotion/react": "^11.1.5",
    "@emotion/serialize": "^1.0.1",
    "@emotion/styled": "^11.1.5",
    "clsx": "^1.1.1",
    "framer-motion": "^4.0.2",
    "js-cookie": "^2.2.1",
    "lodash.merge": "^4.6.2",
    "next": "10.0.9",
    "polished": "^4.1.1",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "sass": "^1.32.12"
  },
  "devDependencies": {
    "@emotion/babel-plugin": "^11.2.0",
    "@types/glob": "^7.1.3",
    "@types/hyphen": "^1.6.3",
    "@types/js-cookie": "^2.2.6",
    "@types/lodash.merge": "^4.6.6",
    "@types/react-dom": "^17.0.3",
    "@typescript-eslint/eslint-plugin": "^4.19.0",
    "@zerollup/ts-transform-paths": "^1.7.18",
    "concurrently": "^6.0.2",
    "eslint": "^7.22.0",
    "eslint-config-airbnb-typescript": "^12.3.1",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.23.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "glob": "^7.1.6",
    "hyphen": "^1.6.2",
    "json-to-ts": "^1.7.0",
    "markdown-to-jsx": "^7.1.2",
    "pretty-format": "^26.6.2",
    "ts-node": "^9.1.1",
    "ttypescript": "^1.5.12",
    "typed-scss-modules": "^4.1.1",
    "typescript": "^4.2.3",
    "webpack-license-plugin": "^4.1.2",
    "yaml": "^2.0.0-4"
  }
}
