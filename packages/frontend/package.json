{
  "name": "frontend-next",
  "version": "0.1.0",
  "author": "Silas Rosenkranz",
  "license": "AGPL-3.0-only",
  "scripts": {
    "dev": "concurrently 'pnpm dev:scss-types' 'pnpm dev:next' 'pnpm dev:i18n'",
    "dev:next": "next dev",
    "dev:scss-types": "tsm --watch src --aliasPrefixes.@ src --nameFormat param --exportType default",
    "dev:i18n": "nodemon --watch src/lib/i18n/locales -e yaml,md --exec 'pnpm build:i18n'",
    "build": "pnpm build:i18n && pnpm build:scss-types && pnpm build:next",
    "build:next": "next build",
    "build:i18n": "pnpm ts-node src/lib/i18n/generator/index.ts",
    "build:scss-types": "tsm src --aliasPrefixes.@ src --nameFormat param --exportType default",
    "start": "next start",
    "lint": "eslint src/**/*.{js,ts,jsx,tsx}",
    "ts-node": "TS_NODE_COMPILER_OPTIONS='{\"module\":\"commonjs\"}' ts-node --compiler ttypescript"
  },
  "dependencies": {
    "@feedbax/api": "workspace:^0.0.1",
    "@feedbax/common": "workspace:^0.0.1",
    "@feedbax/prisma": "workspace:^0.0.1",
    "clsx": "^1.1.1",
    "framer-motion": "^4.1.17",
    "immer": "^9.0.2",
    "js-cookie": "^2.2.1",
    "lodash.merge": "^4.6.2",
    "next": "v10.2.1-canary.5",
    "normalize.css": "^8.0.1",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "sass": "^1.32.13",
    "slugify": "^1.5.3",
    "zustand": "^3.5.1"
  },
  "devDependencies": {
    "@types/glob": "^7.1.3",
    "@types/hyphen": "^1.6.4",
    "@types/js-cookie": "^2.2.6",
    "@types/lodash.merge": "^4.6.6",
    "@types/react-dom": "^17.0.5",
    "@typescript-eslint/eslint-plugin": "^4.24.0",
    "concurrently": "^6.1.0",
    "eslint": "^7.26.0",
    "eslint-config-airbnb-typescript": "^12.3.1",
    "eslint-plugin-import": "^2.23.2",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.23.2",
    "eslint-plugin-react-hooks": "^4.2.0",
    "glob": "^7.1.7",
    "hyphen": "^1.6.4",
    "json-to-ts": "^1.7.0",
    "license-webpack-plugin": "^2.3.18",
    "markdown-to-jsx": "^7.1.2",
    "nodemon": "^2.0.7",
    "pretty-format": "^26.6.2",
    "ts-node": "^9.1.1",
    "typed-scss-modules": "^4.1.1",
    "yaml": "^2.0.0-5"
  }
}
