    "espree": "^7.0.0",
    "estraverse": "^5.1.0",
    "jsdoc": "^3.6.3",
    "minimist": "^1.2.0",
    "protobufjs": "^6.10.1",
    "semver": "^7.3.5",
    "tmp": "^0.2.0",
    "uglify-js": "^3.13.2"
  }
}
