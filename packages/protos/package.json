  "main": "dist/protos.js",
  "types": "dist/protos.d.ts",
  "scripts": {
    "build": "pnpm run build:js && pnpm run build:ts",
    "build:js": "pbjs -t static-module -w commonjs -o dist/protos.js $(find ./src -name '*.proto')",
