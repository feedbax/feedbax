{
  "name": "@feedbax",
  "image": "node:12.19.0-stretch",
  "forwardPorts": [3000],
  "postCreateCommand": "echo \"`/sbin/ip route|awk '/default/ { print $3 }'`\tdocker.host.internal\" | tee -a /etc/hosts > /dev/null",

  "extensions": [
    "zxh404.vscode-proto3",
    "dbaeumer.vscode-eslint",
    "prisma.prisma"
  ],

  "settings": {
    "terminal.integrated.shell.linux": "/bin/bash"
  }
}