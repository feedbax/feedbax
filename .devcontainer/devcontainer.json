{
  "name": "@feedbax",
  "image": "node:12.19.0-stretch",
  "forwardPorts": [3000],

  "postCreateCommand": "sh ./.devcontainer/setdockerhost.sh",
  "postStartCommand": "sh ./.devcontainer/setdockerhost.sh",
  "postAttachCommand": "sh ./.devcontainer/setdockerhost.sh",

  "extensions": [
    "zxh404.vscode-proto3",
    "dbaeumer.vscode-eslint",
    "prisma.prisma",
    "jpoissonnier.vscode-styled-components"
  ],

  "settings": {
    "terminal.integrated.shell.linux": "/bin/bash"
  }
}