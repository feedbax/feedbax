{
	"name": "@feedbax/terminals",
	"description": "Opens named terminals for the project packages.",
	"publisher": "feedbax",
	"version": "0.0.1",
	"main": "./extension.js",

	"engines": {
		"vscode": "^1.56.0"
	},

  "activationEvents": [
		"onCommand:feedbax.terminals.open",
		"onCommand:feedbax.terminals.reset"
	],

	"contributes": {
		"commands": [
			{
				"command": "feedbax.terminals.open",
				"title": "Feedbax Terminals: Open"
			},
			{
				"command": "feedbax.terminals.reset",
				"title": "Feedbax Terminals: Reset"
			}
		]
	}
}